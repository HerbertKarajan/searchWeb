<script src='<!--# include virtual="/n/common/a02/script.html"-->,/gmlib/unit/live800/2.0.0/live800.min.js,/gmlib/unit/scode/1.0.0/scode.min.js,/gmlib/unit/bigdata/1.0.0/bigdata.min.js,/gmlib/unit/scodecommon/1.0.0/scodecommon.min.js'></script>
<script type="text/javascript">
$(function(){
    window.prdInfo = "undefined"
    var _bdarea = $.cookie("atgregion").split("|")[0] || "11010200"
    var BigD = {}
    BigD.tpl_quessLike = function(){
        return '\
            {{each lst as value index}}\
                {{if index<18}}\
                    <li class="item">\
                        <p class="item-pic"><a class="bigD_item" href="{{value.purl}}" target="_blank"><img src="{{value.iurl}}"></a></p>\
                        <p class="item-name"><a class="bigD_item" href="{{value.purl}}" target="_blank">{{value.pn}}</a></p>\
                        <p class="item-price-comment"><b class="item-price">¥{{value.price}}</b></p>\
                    </li>\
                {{/if}}\
            {{/each}}\
        '
    }


    /*bigd 底部猜你喜欢**/
    BigD.quessLike = function(){
        $.ajax({
            type:'get',
            url:"//bigd.gome.com.cn/gome/rec",
            data:{
                "boxid":"box49",
                "pid":"",
                "cid":$.cookie("__clickidc") || "146485295358834057",
                "uid":"",
                "area":_bdarea,
                "imagesize":"100",
                "shopid":"",
                "c1id":"",
                "c3id":"",
                "sid":""
            },
            dataType:'jsonp',
            jsonpName:"rec",
            jsonpCallback:"rec",
            timeout:5000,
            beforeSend:function(){
            },
            success:function(data){
                BigD.renderQuessLike(data)
            }
        })
    }


    BigD.renderQuessLike = function(data){
        if (data.lst && data.lst.length > 0) {
            var listTpl = templateSimple.compile(BigD.tpl_quessLike())(data);
            var _length = data.lst.length
            $("#bigD_quessLike").empty().append(listTpl);
            $("#bigD_quessLike").parent().show();

                if(_length<=5){$("#quessYouLike-refresh").hide()}
                var totlnum = (_length %5 ==0)?(parseInt(_length /5,10)-1):parseInt(_length /5,10);
                var _i = 0;
                $("#bigD_quessLike").find(".item").each(function(){
                    if($(this).index()<5){
                        $(this).addClass("cShow");
                    }
                    $(this).addClass("item"+parseInt($(this).index()/5,10))
                })
                $("#quessYouLike-refresh").bind("click",function(){
                    $("#bigD_quessLike").find(".item").removeClass("cShow");
                    if(_i++ == totlnum || _i==3){
                        _i=0;
                    }
                    $("#bigD_quessLike").find(".item"+_i).addClass("cShow")
                })
        }
    }


    BigD.quessLike()   
})
</script>
